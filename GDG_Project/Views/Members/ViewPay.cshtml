@model IEnumerable<GDG_Project.Models.Payment>


        @{
            ViewData["Title"] = "ViewPay";
            Layout = "~/Views/Shared/_BackeLayout.cshtml";
        }

        <div class="content-wrapper">
            <!-- Main content -->
            <section class="content">
                <div class="row">
                    <div class="col-xs-12">
                        <div class="box">
                            <div class="box-header">
                                <h3 class="box-title">@ViewBag.SchoolName.SchoolName</h3>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body">

                                <table id="example1" class="table table-bordered table-striped">
                                    <thead>
                                        <tr>
                                          
                                            <th>رقم الايصال</th>
                                            <th>تاريخ الدفع </th>
                                            <th>تاريخ التجديد</th>
                                        </tr>
                                    </thead>
                                    <tbody>



                                        @foreach (var item in Model)
                                        {
                                           <tr>
                                           
                                           <td>@item.PayId</td>
                                            <td>@item.PayDate.ToString("dd/MM/yyyy")</td>
                                           <td> @item.PayDueDate.ToString("dd/MM/yyyy")</td>
                                            </tr>
                                        }



                                    </tbody>

                                </table>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>
                    <!-- /.col -->
                </div>
                <!-- /.row -->
            </section>
            <!-- /.content -->
        </div>

       